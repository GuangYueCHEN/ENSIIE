#une fonction pour montrer les 4 resultats:moyenne ,variance,skewness,kurtosis
calcul <- function(x) {
moyenne=sum(x)/100
m=(x-moyenne)^2
variance=sqrt(sum(m)/100)
matrix <- matrix(c(moyenne,variance,skewness(x),kurtosis(x)), nrow = 4)
}
